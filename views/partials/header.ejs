<header id="top-header">
  <span class="siteName">
    <a href="/" title="Return to homepage">CSE Motors</a>
  </span>
  <div id="tools">
    <%  if(!locals.loggedin){ %>
    <a title="Click to log in" href="/account/login">My Account</a>
    <%  } else { %>
    <a href="/account/" title="Go to Account Management">Welcome <%- locals.accountData.account_firstname %></a>
    <a title="Click to log out" href="/account/logout">Logout</a>
    <%  } %>
  </div>
</header>
